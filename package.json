{
  "name": "acip-gpu-gateway",
  "version": "0.1.0",
  "description": "ACIP-based API Gateway for Distributed GPU Inference Platform",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "start": "node dist/index.ts",
    "lint": "eslint src --ext .ts",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "acip",
    "gpu",
    "inference",
    "gateway",
    "api-gateway",
    "distributed"
  ],
  "author": "ACIP Team",
  "license": "MIT",
  "dependencies": {
    "@maxeven/acip-core": "^0.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "helmet": "^8.0.0",
    "http-proxy-middleware": "^3.0.3",
    "ioredis": "^5.4.2",
    "prom-client": "^15.1.3",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.10.0",
    "@types/uuid": "^10.0.0",
    "eslint": "^9.16.0",
    "jest": "^30.0.5",
    "ts-jest": "^29.4.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
